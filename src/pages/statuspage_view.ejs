   <body style="box-shadow: 5px 10px #f5f8fa" class="status-page ">
         <style type="text/css">
         body.status-page {
         background-color: #f5f8fa;
         color: #333333;
         }
         p, strong { color: #333333 !important; }
         .reds { color: #ff6f6f !important; }
         .blues { color: #3498db !important; }
         .greens { color: #50cd89 !important; }
         .yellows { color: #F7CA18 !important; }
         .oranges { color: #FF8800 !important; }
         .metrics { color: #0dccc0 !important; }
         .links { color: #50cd89 !important; }
         /**
         * Banner background
         */
         .app-banner-padding {
         padding: 40px 0 !important;
         }
         /**
         * Alert overrides.
         */
         .alert {
         background-color: #F7CA18;
         border-color: #deb515;
         color: white;
         }
         .alert.alert-success {
         background-color: #50cd89;
         border-color: #50cd89;
         color: white;
         }
         .alert.alert-info {
         background-color: #3498db;
         border-color: #2e88c5;
         color: white;
         }
         .alert.alert-danger {
         background-color: #ff6f6f;
         border-color: #e56363;
         color: white;
         }
         /**
         * Button Overrides
         */
         .btn.links {
         color: #ac8d10;
         }
         .btn.btn-success {
         background-color: #50cd89;
         border-color: #50cd89;
         color: white;
         }
         .btn.btn-success.links {
         color: #50cd89;
         }
         .btn.btn-success.btn-outline {
         background-color: transparent;
         border-color: #50cd89;
         color: #50cd89;
         }
         .btn.btn-success.btn-outline:hover {
         background-color: #50cd89;
         border-color: #50cd89;
         color: white;
         }
         .btn.btn-info {
         background-color: #3498db;
         border-color: #2e88c5;
         color: white;
         }
         .btn.btn-info.links {
         color: #246a99;
         }
         .btn.btn-danger {
         background-color: #ff6f6f;
         border-color: #e56363;
         color: white;
         }
         .btn.btn-danger.links {
         color: #b24d4d;
         }
         /**
         * Background fills Overrides
         */
         .component {
         background-color: #FFFFFF;
         border-color: #e5e5e5;
         }
         .sub-component {
         background-color: #FFFFFF;
         border-color: #e5e5e5;
         }
         .incident {
         background-color: #FFFFFF;
         border-color: #e5e5e5;
         }
         .status-icon {
         background-color: #FFFFFF;
         border-color: #e5e5e5;
         }
         .panel.panel-message:before {
         border-left-color: #FFFFFF !important;
         border-right-color: #FFFFFF !important;
         }
         .panel.panel-message:after {
         border-left-color: #FFFFFF !important;
         border-right-color: #FFFFFF !important;
         }
         .footer a {
         color: #333333;
         }
         img {
         -webkit-user-select: none; /* Safari */
         -ms-user-select: none; /* IE 10 and IE 11 */
         user-select: none; /* Standard syntax */
         }
      </style>
      <div class="container">
         <div class="section-messages">
         </div>
         <center><a href="/"><img draggable="false" width="220" src="https://i.ibb.co/Mgq22LW/Apptime-new-Branding.png"></a></center>
         <br>
         <div class="section-status">
         <%if(bot.ping > 300) {%>
            <div style="font-size:22px" class="alert alert-danger"><strong><as style="color:white">System Low Performance</as></strong></div>
            <% } else { %>
            <div style="font-size:22px" class="alert alert-success"><strong><as style="color:white">All systems are operational</as></strong></div>
            <% } %>
         </div>
         <div class="about-app" style="margin-top:4%">
            <h3><strong>About This Site</strong></h3>
            <p style="margin-top:-10px"><%=ly.fetch(`statuspage.${ly.fetch("linkler").filter(x => x.pointID == link)[0].pointID}.about`) || `Status Page for ${ly.fetch("linkler").filter(x => x.pointID == link)[0].name}`%> <%if(user) {%><%if (ly.fetch("linkler").filter(x => x.url === linkasil)[0].owner ===
        user.id) {%><i><a style="font-size:14px" href="/monitors/view/<%=ly.fetch("linkler").filter(x => x.pointID == link)[0].pointID%>">Edit About</a></i><%}%><%}%></p>
         </div>
         <ul style="box-shadow: 5px 10px #f5f8fa" class="list-group">
            <li style="background-color:#ffffff;border:1px solid #ffffff;box-shadow: 5px 10px #f5f8fa;border-radius:10px;border-bottom-left-radius:0px;border-bottom-right-radius:0px;" class="list-group-item group-name">
               <strong class="text-uppercase">General</strong>
            </li>
            <div class="group-items ">
               <li style="background-color:#ffffff;border:1px solid #ffffff;border-radius:10px;border-top-left-radius:0px;border-top-right-radius:0px;" class="list-group-item sub-component">
                  <%=ly.fetch("linkler").filter(x => x.pointID == link)[0].name%>
                  <div class="pull-right">
                  <%if(bot.ping > 300) {%>
                     <small class="text-component-1 reds">Not Operational</small>
                  <% } else {%>
                  <small class="text-component-1 greens">Operational</small>
                  <% } %>
                  </div>
               </li>
            </div>
         </ul>
         <ul style="box-shadow: 5px 10px #f5f8fa" class="list-group">
            <li style="background-color:#ffffff;border:1px solid #ffffff;border-radius:10px;border-bottom-left-radius:0px;border-bottom-right-radius:0px;" class="list-group-item group-name">
               <strong class="text-uppercase">Third-party</strong>
            </li>
            <li style="background-color:#ffffff;border:1px solid #ffffff" class="list-group-item component">
               Apptime API
               <div class="pull-right">
                  <%if(bot.ping > 300) {%>
                     <small class="text-component-1 reds">Not Operational</small>
                  <% } else {%>
                  <small class="text-component-1 greens">Operational</small>
                  <% } %>
               </div>
            </li>
            <li style="background-color:#ffffff;border:1px solid #ffffff" class="list-group-item component">
               Apptime Status Pages
               <div class="pull-right">
                  <%if(bot.ping > 300) {%>
                     <small class="text-component-1 reds">Not Operational</small>
                  <% } else {%>
                  <small class="text-component-1 greens">Operational</small>
                  <% } %>
               </div>
            </li>
            <li style="background-color:#ffffff;border:1px solid #ffffff" class="list-group-item component">
               Apptime Monitoring
               <div class="pull-right">
                  <%if(bot.ping > 300) {%>
                     <small class="text-component-1 reds">Not Operational</small>
                  <% } else {%>
                  <small class="text-component-1 greens">Operational</small>
                  <% } %>
               </div>
            </li>
            <li style="background-color:#ffffff;border:1px solid #ffffff" class="list-group-item component">
               Google Cloud Service
               <div class="pull-right">
                  <%if(bot.ping > 600) {%>
                     <small class="text-component-1 reds">Not Operational</small>
                  <% } else {%>
                  <small class="text-component-1 greens">Operational</small>
                  <% } %>
               </div>
            </li>
            <li style="background-color:#ffffff;border:1px solid #ffffff;border-radius:10px;border-top-left-radius:0px;border-top-right-radius:0px;" class="list-group-item component">
               Cloudflare
               <div class="pull-right">
                  <small class="text-component-1 greens">Operational</small>
               </div>
            </li>
         </ul>
      </div>
      </div>
      <footer class="footer">
         <div class="container">
            <div class="row">
               <div class="col-sm-5">
                  <p>Status Pages â‹… Powered by <a style="color:#20aee3" href="/">Apptime</a> for <%=ly.fetch("linkler").filter(x => x.pointID == link)[0].name%></p>
               </div>
               <div class="col-sm-7">
                  <ul class="list-inline">
                     <li>
                        <a class="btn btn-link" href="/legal/gdpr" target="_blank">GDPR</a>
                     </li>
                     <li>
                        <a class="btn btn-link" href="/legal/privacy" target="_blank">Privacy Policy</a>
                     </li>
                     <li>
                        <a class="btn btn-link" href="/legal/tos" target="_blank">Terms of Service</a>
                     </li>
                  </ul>
               </div>
            </div>
         </div>
      </footer>
   </body>
</html>